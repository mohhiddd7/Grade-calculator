try:
    grade = float(input("Enter your percentage here: "))
    
    if grade >= 90 and grade <= 100:
        print("Your grade is A+")
    elif grade >= 87 and grade <= 89:
        print("Your grade is A")
    elif grade >= 80 and grade <= 86:
        print("Your grade is B+")
    elif grade >= 70 and grade <= 79:
        print("Your grade is B")
    elif grade >= 60 and grade <= 69:
        print("Your grade is C")
    elif grade >= 50 and grade <= 59:
        print("Your grade is D")
    elif grade >= 0 and grade <= 49:
        print("You have Failed")
    else:
        print("Enter a valid percentage (0-100)")

except ValueError:
    print("Enter a valid number, not letters or symbols!")
